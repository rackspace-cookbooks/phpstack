{
   "env_run_lists" : {},
   "run_list" : [ "recipe[phpstack::feature_flags]" ],
   "json_class" : "Chef::Role",
   "chef_type" : "role",
   "override_attributes" : {},
   "name" : "feature-flags",
   "description" : "",
   "default_attributes" : {
     "rackspace": {
        "cloud_credentials": {
          "username": "secretusername",
          "api_key": "secretapikey"
        }
      },
      "newrelic": {
       "licence" : "secretlicence"
      },
      "platformstack": {
        "cloud_monitoring": {
          "enabled": true
        }
      },
      "phpstack": {
        "flags" : {
          "monitoring": {
            "enabled": true,
            "newrelic": true,
            "cloudmonitoring": true
          },
          "webserver": {
            "enabled": true,
            "apache": true
          },
          "application": { "enabled": true },
          "mysql": {
            "enabled": true,
            "standalone": true
          }
        },
        "demo": {
          "enabled": true
        }
      }
   }
}
